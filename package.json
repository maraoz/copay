    "buffertools": "2.0.1",
    "chai": "1.9.1",
    "commander": "2.1.0",
    "coveralls": "2.10.0",
    "express": "4.0.0",
    "grunt-browserify": "2.0.8",
    "grunt-contrib-watch": "0.5.3",
