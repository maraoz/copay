    "mocha": "1.18.2",
    "mocha-lcov-reporter": "0.0.1",
    "node-cryptojs-aes": "0.4.0",
    "sinon": "1.9.1",
    "soop": "0.1.5",
    "travis-cov": "0.2.5",
